    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.16",
    "@mui/styles": "^5.10.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.4.1",
    "typescript": "^4.8.3",
    "web-vitals": "^3.0.2"
  },
  "devDependencies": {
